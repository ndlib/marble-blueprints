{
  "name": "marble-images-blueprints",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndlib/marble-images-blueprints.git"
  },
  "bugs": {
    "url": "https://github.com/ndlib/marble-images-blueprints/issues"
  },
  "homepage": "https://github.com/ndlib/marble-images-blueprints#readme",
  "keywords": [
    "aws",
    "cdk",
    "ndlib"
  ],
  "bin": {
    "deploy": "bin/deploy.js"
  },
  "scripts": {
    "cdk": "cdk",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"bin/**/*.ts\" \"bin/**/*.js\" \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git checkout -B bump-$npm_package_version && git add -A src",
    "postversion": "git push --set-upstream origin bump-$npm_package_version && git push --tags",
    "watch": "tsc-watch --onSuccess \"npm run test --  --watch\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.49.1",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "aws-cdk": "1.49.1",
    "jest": "^25.5.4",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "tsc-watch": "^4.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-codepipeline-actions": "1.49.1",
    "@aws-cdk/aws-ec2": "1.49.1",
    "@aws-cdk/aws-events-targets": "1.49.1",
    "@aws-cdk/aws-lambda-event-sources": "1.49.1",
    "@aws-cdk/aws-lambda": "1.49.1",
    "@aws-cdk/aws-s3": "1.49.1",
    "@aws-cdk/custom-resources": "1.49.1",
    "@aws-cdk/core": "1.49.1",
    "@ndlib/ndlib-cdk": "^1.2.2",
    "source-map-support": "^0.5.16"
  },
  "files": [
    "lib/**/*"
  ]
}
